{
  "name": "bah",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "watchify -vd -p browserify-hmr -t babelify -t vueify -e src/main.js -o public/javascripts/bundle.js",
    "build": "cross-env NODE_ENV='production' browserify -g envify -t babelify -t vueify -e src/main.js -o public/javascripts/bundle.js",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.15.2",
    "browserify-hmr": "^0.3.5",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.1.1",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "envify": "^4.1.0",
    "express": "~4.14.0",
    "kue": "^0.11.6",
    "mongoose": "^4.12.4",
    "mongoose-slug-hero": "^1.0.8",
    "morgan": "~1.7.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "pug": "~2.0.0-beta6",
    "sass-loader": "^6.0.6",
    "serve-favicon": "~2.3.0",
    "slug": "^0.9.1",
    "socket.io": "^2.0.4",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  }
